    <div class="mb-5 pt-3">
        <a href="/dashboard/create/">
            <button id="create-post" class="button is-primary is-pulled-right">create post</button>
        </a>
        
        <h2 class="title pb-4">Dashboard: your posts</h2>
{{#if posts.length}}
    </div>

    {{#each posts as |post|}}
        {{> post-info post}}
        
        <div class="button-is-warning-div">
            <a href="/dashboard/edit/{{post.id}}" class="button-is-warning-div"> 
            <button class="button mb-5 ml-5 is-warning">edit post</button></a> </div>
        </div>
    {{/each}}

{{else}}
<h6 class="pb-4 has-text-centered is-italic">No Posts found</h6>

{{/if}}


